	&& echo "eba5c51f657c1b05d5930475d1723758cd86db74499125ab48f0f9d1863845f7  go$GO_VERSION.linux-386.tar.gz" | sha256sum -c - \
	&& tar -xzf "go$GO_VERSION.linux-386.tar.gz" -C /usr/local/go --strip-components=1 \
