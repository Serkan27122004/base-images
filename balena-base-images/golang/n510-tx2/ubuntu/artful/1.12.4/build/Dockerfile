	&& echo "858bba9fb3293f459f39443359b6ee839a2f7ba264f23966a0b15fbbdc1156df  go$GO_VERSION.linux-aarch64.tar.gz" | sha256sum -c - \
	&& tar -xzf "go$GO_VERSION.linux-aarch64.tar.gz" -C /usr/local/go --strip-components=1 \
	&& rm -f go$GO_VERSION.linux-aarch64.tar.gz

