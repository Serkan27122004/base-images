	&& echo "ad719c8f660323ddd685836cd7a2b71c0c3abd68af1716d1767c1cf8a3f19883  go$GO_VERSION.linux-armel.tar.gz" | sha256sum -c - \
	&& tar -xzf "go$GO_VERSION.linux-armel.tar.gz" -C /usr/local/go --strip-components=1 \
	&& rm -f go$GO_VERSION.linux-armel.tar.gz

